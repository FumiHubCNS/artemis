source @ROOT_DIR@/../bin/thisroot.csh

setenv LD_LIBRARY_PATH @yaml_cpp_LIBDIR@:$LD_LIBRARY_PATH
setenv ARTSYS @CMAKE_INSTALL_PREFIX@
setenv PATH $ARTSYS/bin:$PATH
setenv LD_LIBRARY_PATH $ARTSYS/lib:$LD_LIBRARY_PATH

if [ "@BUILD_GET@" == "ON" ]; then 
    setenv LD_LIBRARY_PATH @GET_LIB_DIR@:$LD_LIBRARY_PATH
fi

if [ "@MPI_FOUND@" == "TRUE" ]; then 
    dir=@MPI_CXX_LIBRARIES@
    libdir=`dirname $dir`
    setenv LD_LIBRARY_PATH $libdir:$LD_LIBRARY_PATH
fi